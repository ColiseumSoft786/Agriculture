<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\DataBundle\Entity\Question" table="question">
    <id name="id" type="integer" column="Id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="question" type="text" column="Question" nullable="true"/>
    <field name="name" type="string" column="Name" length="45" nullable="true"/>
    <field name="email" type="string" column="Email" length="100" nullable="true"/>
    <field name="approved" type="integer" column="Approved" nullable="true"/>
    <field name="date" type="datetime" column="Date" nullable="true"/>
  </entity>
</doctrine-mapping>
