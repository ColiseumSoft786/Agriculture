<!DOCTYPE html>
<html lang="en-US" dir="ltr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--
    Document Title
    =============================================
    -->
    <title>Fresh Vegetables | {% block title %}{% endblock %}</title>


    <link href="{{ asset('bundles/assets/lib/bootstrap/dist/css/bootstrap.min.css') }}" rel="stylesheet">
    <!-- Template specific stylesheets-->
    <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed:400,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Volkhov:400i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800" rel="stylesheet">

    <link href="{{ asset('bundles/assets/lib/animate.css/animate.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/assets/lib/components-font-awesome/css/font-awesome.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/assets/lib/et-line-font/et-line-font.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/assets/lib/flexslider/flexslider.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/assets/lib/owl.carousel/dist/assets/owl.carousel.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/assets/lib/owl.carousel/dist/assets/owl.theme.default.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/assets/lib/magnific-popup/dist/magnific-popup.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/assets/lib/simple-text-rotator/simpletextrotator.css') }}" rel="stylesheet">
    <!-- Main stylesheet and color file-->
    <link href="{{ asset('bundles/assets/css/style.css') }}" rel="stylesheet">
    <link id="color-scheme" href="{{ asset('bundles/assets/css/colors/default.css') }}" rel="stylesheet">
    {% block stylesheets %}{% endblock %}

</head>
<body data-spy="scroll" data-target=".onpage-navigation" data-offset="60">
<main>
    <div class="page-loader">
        <div class="loader">Loading...</div>
    </div>
    <nav class="navbar navbar-custom navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#custom-collapse"><span class="sr-only">Toggle navigation</span><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></button><a class="navbar-brand" href="{{ path('home_homepage') }}">Fresh Vegetables</a>
            </div>
            <div class="collapse navbar-collapse" id="custom-collapse">
                {% render(controller('HomeBundle:Default:menu'))%}

            </div>
        </div>
    </nav>
    <div class="main">
        <section class="module bg-dark-60 blog-page-header" style="padding: 50px 0" data-background="{{ asset('bundles/assets/images/main.jpg') }}">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6 col-sm-offset-3">
                        <h2 style="margin-bottom: 10px" class="module-title font-alt">{{ block('title') }}</h2>
                    </div>
                </div>
            </div>
        </section>
        {% block body %}{% endblock %}

        <footer class="footer bg-dark">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <p class="copyright font-alt">&copy; 2017&nbsp;<a href="">Agriculture</a>, All Rights Reserved</p>
                    </div>
                    <div class="col-sm-6">
                        <div class="footer-social-links"><a href="#"><i class="fa fa-facebook"></i></a><a href="#"><i class="fa fa-twitter"></i></a><a href="#"><i class="fa fa-dribbble"></i></a><a href="#"><i class="fa fa-skype"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>
    <div class="scroll-up"><a href="#totop"><i class="fa fa-angle-double-up"></i></a></div>
</main>
<!--
JavaScripts
=============================================
-->
<script src="{{ asset('bundles/assets/lib/jquery/dist/jquery.js') }}"></script>
<script src="{{ asset('bundles/assets/lib/bootstrap/dist/js/bootstrap.min.js') }}"></script>
<script src="{{ asset('bundles/assets/lib/wow/dist/wow.js') }}"></script>
<script src="{{ asset('bundles/assets/lib/jquery.mb.ytplayer/dist/jquery.mb.YTPlayer.js') }}"></script>
<script src="{{ asset('bundles/assets/lib/isotope/dist/isotope.pkgd.js') }}"></script>
<script src="{{ asset('bundles/assets/lib/imagesloaded/imagesloaded.pkgd.js') }}"></script>
<script src="{{ asset('bundles/assets/lib/flexslider/jquery.flexslider.js') }}"></script>
<script src="{{ asset('bundles/assets/lib/owl.carousel/dist/owl.carousel.min.js') }}"></script>
<script src="{{ asset('bundles/assets/lib/smoothscroll.js') }}"></script>
<script src="{{ asset('bundles/assets/lib/magnific-popup/dist/jquery.magnific-popup.js') }}"></script>
<script src="{{ asset('bundles/assets/lib/simple-text-rotator/jquery.simple-text-rotator.min.js') }}"></script>
<script src="{{ asset('bundles/assets/js/plugins.js') }}"></script>
<script src="{{ asset('bundles/assets/js/main.js') }}"></script>
{% block script %}{% endblock %}
</body>
</html>